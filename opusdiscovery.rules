
(rules PCB opusdiscovery
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 2590)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 200.0)
    (clear 200.2)
    (clear 100.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-power" "Via[0-1]_800:400_um" power
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    power "Via[0-1]_800:400_um-power"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    /RESET "Net-(D2-Pad2)" /A11 /A9 /A4 /A6 /A7 /A10
    /A8 /M1 /WR /A3 /A2 /A1 /A0 /A12
    /A14 /MREQ NMI /A13 /A15 "FDC_CS" "ZX_ROMOE1" "ZX_ROMOE2"
    "ZX_ROMOE48" "ROM_A14" "ROM_A13" "J1-Up" "J2-Down" "J3-Left" "J4-Right" "J6-Fire"
    /A5 /RD /IOREQ /D4 /D3 /D5 /D6 /D2
    /D1 /D0 /D7 /DS0 /DS1 "RAM_CS" "ROM_CS" "Net-(D5-Pad2)"
    /INDEX "/MOTOR_ON" /DIR /STEP /HEAD "/TRACK_0" "Net-(C13-Pad2)" "Net-(C13-Pad1)"
    "Net-(C14-Pad1)" "Net-(C14-Pad2)" "Net-(D3-Pad2)" "Net-(IC1-Pad25)" "Net-(IC1-Pad22)" "Net-(IC1-Pad21)" "Net-(IC1-Pad20)" "Net-(IC1-Pad19)"
    "Net-(IC1-Pad17)" "Net-(IC1-Pad16)" "Net-(IC5-Pad2)" "Net-(IC5-Pad3)" "Net-(IC5-Pad12)" "Net-(IC9-Pad4)" "Net-(J2-Pad10)" "Net-(J2-Pad14)"
    "JOY_EN" "Net-(IC1-Pad27)" DDEN "CX_J" "CX_K" "CX_CK" "CX_U6A_A1" /RFSH
    "S_U12A" "Net-(J3-Pad1)" "Net-(J3-Pad3)" "Net-(J3-Pad5)" "Net-(J3-Pad7)" "Net-(J3-Pad9)" "Net-(J3-Pad11)" "Net-(J3-Pad13)"
    "Net-(J3-Pad15)" "Net-(J3-Pad17)" "Net-(J3-Pad19)" "Net-(J3-Pad21)" "ZX_I_ROMOE" "Net-(JP5-Pad3)" "Net-(JP5-Pad2)" "Net-(U1-Pad8)"
    "PIA_CS" "Net-(U2-Pad15)" "Net-(U10-Pad1)" "Net-(U10-Pad4)" "Net-(U10-Pad9)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class power
    GND +9V +5V "plus3_5V" "Net-(IC12-Pad3)"
    (clearance_class power)
    (via_rule power)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)